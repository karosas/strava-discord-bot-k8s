            "in": "path",
            "name": "leaderboardId",
            "required": true,
            "type": "string"
          },
          {
            "in": "path",
            "name": "participantId",
            "required": true,
            "type": "string"
          },
          {
            "in": "query",
            "name": "from",
            "type": "string",
            "format": "date-time"
